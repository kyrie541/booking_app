<% include partials/header %>

<h1>Sign up Form</h1>

<form action="/register" method="POST">
    <input type="text" name="username" placeholder="username">
    <input type="password" name="password" placeholder="password">
    <br>
    <br>
    <label for="location">Outlet: </label>
    <select name="location">
      <option value="SAS">Sports Arena Sentosa</option>
      <option value="SABS">Sports Arena Bukit Serdang</option>
      <option value="SAMC">Sports Arena Mahkota Cheras</option>
      <option value="SAP">Sports Arena Puchong</option>
    </select>
    <button>Submit</button>
</form>

<li><a href="/login">Login!</a></li>

